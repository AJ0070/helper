{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "local": true,
  "remote": {
    "urls": ["http://localhost:3010", "https://helperai.dev", "https://helper.ai"]
  },
  "permissions": [
    "core:default",
    "core:event:default",
    "opener:default",
    "os:default",
    "deep-link:default",
    "core:window:default",
    "core:window:allow-start-dragging"
  ]
}
